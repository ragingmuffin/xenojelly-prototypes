#include <Servo.h>

Servo tentacle1;  // create servo objects to control servos   up to twelve servo objects can be created on most boards
Servo tentacle2;
Servo tentacle3;
Servo tentacle4;
Servo tentacle5;
Servo tentacle6;

int pos = 0;    // variable to store the servo position

 
void setup() {
  tentacle1.attach(10);  // attaches the servo on pins to the servo objects
  tentacle2.attach(9);
  tentacle3.attach(8);
  tentacle4.attach(7);
  tentacle5.attach(6);
  tentacle6.attach(5);
}

void loop() {
  for (pos = 0; pos <= 70; pos += 1) { // goes from 0 degrees to 70 degrees
    // in steps of 1 degree
    tentacle1.write(pos);              // tell servo to go to position in variable 'pos'
    tentacle2.write(pos);
    tentacle3.write(pos);
    tentacle4.write(pos);
    tentacle5.write(pos);
    tentacle6.write(pos);
    delay(30);                       // waits 30ms for the servo to reach the position
  }
  for (pos = 70; pos >= 0; pos -= 1) { // goes from 70 degrees to 0 degrees
    tentacle1.write(pos);              // tell servo to go to position in variable 'pos'
    tentacle2.write(pos);
    tentacle3.write(pos);
    tentacle4.write(pos);
    tentacle5.write(pos);
    tentacle6.write(pos);
    delay(20);                       // waits 20ms for the servo to reach the position
  }
}
